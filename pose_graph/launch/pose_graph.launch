<launch>
    <node pkg="feature_tracker" type="feature_tracker_node" name="feature_tracker" output="screen">
        <param name="img_topic" value="/cam0/image_raw"/>
        <param name="config_file" value="$(find feature_tracker)/config/euroc.yaml"/>
        <param name="freq" value = "10.0"/>
    </node>
    
    <node pkg="vins_estimator" type="vins_estimator_node" name="vins_estimator" output="screen">
        <param name="feature_topic" value="/feature_tracker/feature"/>
        <param name="imu_topic" value="/imu0"/>
    </node>
    <node pkg="pose_graph" type="pose_graph_node" name="pose_graph" output="screen">
    </node>

    <node pkg="odom_recorder" type="odom_recorder_node" name="odom_recorder" output="screen">
        <param name="odom_topic" value="/pose_graph/pose_graph_odom"/>
        <param name="save_text_path" value="/home/zhouzhou/temp/MH_01_LOOP.csv"/>
    </node>
</launch>